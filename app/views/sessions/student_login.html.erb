<%= form_with url: login_path, scope: "login" do |f|%>
    <div>
        <%= f.label :username %>
        <%= f.text_field :username %>
    </div>
    <div>
        <%= f.label :password %>
        <%= f.password_field :password %>
    </div>
    <div>
        <%= link_to "Create a new student", new_student_path %>
    </div>
    <div>
        <%= f.submit "Log In" %>
    </div>
<% end %>